        <activity
            android:name=".Camara"
            android:label="@string/title_activity_camara" >
        </activity>
        <activity
            android:name=".GPS"
            android:label="@string/title_activity_gps" >
        </activity>
        <activity
            android:name=".Speech"
            android:label="@string/title_activity_speech" >
        </activity>
        <activity
            android:name=".TextToSpeechActivity"
            android:label="@string/title_activity_text_to_speech" >
        </activity>
        <activity
            android:name=".Video"
            android:label="@string/title_activity_video" >
        </activity>
    </application>

</manifest>
